.................... // BCD range 00 - 99 
.................... int8 bcd2bin(int8 bcd_value)  
.................... {  
....................   int8 temp; 
....................   temp = bcd_value; 
*
0565:  MOVF   5B,W
0566:  MOVWF  5C
....................   temp &= 0xF0; //get high nibble which store tens value 
0567:  MOVLW  F0
0568:  ANDWF  5C,F
....................   return((temp >> 1) + (temp >> 3) + (bcd_value & 0x0F)); //(tens * 8) + (tens * 2) + ones 
0569:  BCF    03.0
056A:  RRF    5C,W
056B:  MOVWF  5D
056C:  RRF    5C,W
056D:  MOVWF  77
056E:  RRF    77,F
056F:  RRF    77,F
0570:  MOVLW  1F
0571:  ANDWF  77,F
0572:  MOVF   77,W
0573:  ADDWF  5D,F
0574:  MOVF   5B,W
0575:  ANDLW  0F
0576:  ADDWF  5D,W
0577:  MOVWF  78
0578:  RETURN
.................... }  
