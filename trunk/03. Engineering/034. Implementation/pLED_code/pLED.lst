.................... // BCD range 00 - 99 
.................... int8 bcd2bin(int8 bcd_value)  
.................... {  
....................   int8 temp; 
....................   temp = bcd_value; 
*
0564:  MOVF   5A,W
0565:  MOVWF  5B
....................   temp &= 0xF0; //get high nibble which store tens value 
0566:  MOVLW  F0
0567:  ANDWF  5B,F
....................   return((temp >> 1) + (temp >> 3) + (bcd_value & 0x0F)); //(tens * 8) + (tens * 2) + ones 
0568:  BCF    03.0
0569:  RRF    5B,W
056A:  MOVWF  5C
056B:  RRF    5B,W
056C:  MOVWF  77
056D:  RRF    77,F
056E:  RRF    77,F
056F:  MOVLW  1F
0570:  ANDWF  77,F
0571:  MOVF   77,W
0572:  ADDWF  5C,F
0573:  MOVF   5A,W
0574:  ANDLW  0F
0575:  ADDWF  5C,W
0576:  MOVWF  78
0577:  RETURN
.................... }  
